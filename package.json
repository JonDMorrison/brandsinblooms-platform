  "description": "Brands in Blooms platform",
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:all": "node scripts/dev.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "type-coverage": "npx type-coverage --strict",
    "type-coverage:report": "npx type-coverage --strict --detail",
    "analyze": "ANALYZE=true next build",
    "generate-types": "supabase gen types typescript --local > src/lib/database/types.ts",
    "shadcn": "pnpm dlx shadcn-ui@latest",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase db push",
    "supabase:generate-types": "supabase gen types typescript --local > src/lib/database/types.ts",
    "docker:build": "docker build -t brands-in-blooms .",
    "docker:run": "docker run -p 3000:3000 -e PORT=3000 brands-in-blooms",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "node tests/integration/setup.ts && jest tests/integration",
    "test:all": "pnpm test:unit && pnpm test:integration",
    "deploy": "node scripts/deploy.js",
    "seed:demo-content": "node scripts/seed-demo-content.mjs",
    "deploy:staging": "node scripts/deploy.js --env staging",
    "deploy:production": "node scripts/deploy.js --env production",
    "deploy:preview": "node scripts/deploy.js --preview",
    "deployment:status": "node scripts/deployment-status.js",
    "deployment:rollback": "node scripts/rollback.js",
    "test:create-project": "node scripts/test-create-project.js",
    "railway:setup": "bash scripts/railway-setup.sh",
    "railway:env": "node scripts/railway-env.js",
    "railway:deploy": "bash scripts/railway-deploy.sh",
    "railway:deploy:staging": "bash scripts/railway-deploy.sh staging",
    "railway:deploy:production": "bash scripts/railway-deploy.sh production",
    "railway:logs": "railway logs --tail",
    "railway:status": "railway status"
  },
