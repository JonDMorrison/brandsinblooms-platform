{
    "buildCommand": "pnpm run build",
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
    "framework": "nextjs",
    "installCommand": "pnpm install",
    "outputDirectory": ".next",
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    },
    "crons": [
        {
            "path": "/api/cron/process-jobs",
            "schedule": "* * * * *"
        }
    ]
}
